{
  "name": "chronoscape",
  "version": "0.1.0",
  "description": "Interactive 3D Git repository history visualizer",
  "type": "module",
  "private": true,
  "bin": {
    "chronoscape": "./bin/chronoscape.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "export": "node ./bin/chronoscape.js export"
  },
  "keywords": [
    "git",
    "history",
    "3d",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "file:packages/react-three-drei",
    "@react-three/fiber": "file:packages/react-three-fiber",
    "@react-three/postprocessing": "file:packages/react-three-postprocessing",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "three": "0.183.1"
  },
  "devDependencies": {
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/three": "0.183.1",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
